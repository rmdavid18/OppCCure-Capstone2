                                    <td>@patient.FirstName @patient.MiddleName @patient.LastName </td>
                                    <td>@patient.Gender</td>
                                    <td>@patient.BirthDate.Date.ToString("MM/dd/yyyy") </td>
                                    <td>@patient.Address</td>



