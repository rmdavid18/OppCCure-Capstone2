@section Scripts {

    <script type="text/javascript">

        function onScanSuccess(decodedText, decodedResult) {
            // Handle on success condition with the decoded text or result.
            const modal = new bootstrap.Modal("#exampleModal")
            console.log(`Scan result: ${decodedText}`, decodedResult);
            $("#exampleModal").find(".modal-body").text(`${decodedText}`);
            $("a[href='link-here']").attr('href', `${decodedText}`)
            modal.show();
        }

        var html5QrcodeScanner = new Html5QrcodeScanner(
            "reader", { fps: 10, qrbox: 250 });
        html5QrcodeScanner.render(onScanSuccess);
    </script>



    <script type="text/javascript">

        $(document).ready(function () {
            var url = window.location.href;
            var activeTab = url.substring(url.indexOf("#") + 1);
            if (activeTab == "patients" || activeTab == "appts" || activeTab == "user") {
                $(".tab-pane").removeClass("active");
                $("#" + activeTab).addClass("active");
                $(".nav-link").removeClass("active");
                $("#Nav" + activeTab).addClass("active");
                $('a[href="' + activeTab + '"]').tab('show')

            }
        });


        $("#search").keyup(function (event) {
            if (event.keyCode === 13) {
                window.location = "/manage/admin/dashboard?keyword=" + $("#search").val() + "#patients";
            }
        });

        $("#aptssearch").keyup(function (event) {
            if (event.keyCode === 13) {
                window.location = "/manage/admin/dashboard?keyword=" + $("#aptssearch").val() + "#appts";
            }
        });
        $("#usersearch").keyup(function (event) {
            if (event.keyCode === 13) {
                window.location = "/manage/admin/dashboard?keyword=" + $("#usersearch").val() + "#user";
            }
        });
        //user

        $("#usersortBy").change(function () {
            window.location = "/manage/admin/dashboard?keyword=@(Model.View.Pasyente?.Keyword)&sortBy=" + $("#usersortBy").val() + "&sortOrder=" + $("#usersortOrder").val() + "#user";
        });
        $("#usersortOrder").change(function () {
            window.location = "/manage/admin/dashboard?keyword=@(Model.View.Pasyente?.Keyword)&sortBy=" + $("#usersortBy").val() + "&sortOrder=" + $("#usersortOrder").val() + "#user";
        });
        //
