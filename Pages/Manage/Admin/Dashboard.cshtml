            window.location = "/manage/admin/dashboard?keyword=@(Model.View.Pasyente?.Keyword)&sortBy=" + $("#sortBy").val() + "&sortOrder=" + $("#sortOrder").val() + "&status=" + $("#aptsfilterBy").val() + "&date=" + $("#aptsfilterDate").val() + "#patients";
        });
        $("#sortOrder").change(function () {
