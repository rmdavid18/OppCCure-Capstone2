                                                                <div class="form-group">
                                                                    <button type="submit" class="btn btn-primary">Save</button>
                                                                    <a href="~/manage/admin/dashboard" class="btn btn-secondary">Cancel</a>
                                                                </div>



                                                            </form>

                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <!--Modal-->







                                    </td>
                                </tr>
                            }
