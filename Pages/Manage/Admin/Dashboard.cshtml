                        {
                            <li class="page-item"><a class="page-link" href="/manage/admin/dashboard?pageIndex=@(Model?.View?.Pasyente?.PageIndex-1)&pageSize=@(Model?.View?.Pasyente?.PageSize)&sortBy=@(Model?.View?.Pasyente?.SortBy)&sortOrder=@(Model?.View?.Pasyente?.SortOrder)">Previous</a></li>
